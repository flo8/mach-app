{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@models/*": ["src/models/*"],
      "@api/*": ["src/api/*"],
      "@layouts/*": ["src/layouts/*"],
      "@Pagelayout": ["src/layouts/PageLayout.astro"]
    }
  }
}
