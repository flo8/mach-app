---
import Navbar from '@components/Navbar.astro';
import BaseHead from '@components/Head/BaseHead.astro';

export interface Props {
  title: string;
}

const { title, frontmatter } = Astro.props;
---

<html lang='en'>
  <head>
    <BaseHead title={title || frontmatter?.title} />
  </head>
  <body>
    <Navbar />
    <slot />
  </body>
</html>
